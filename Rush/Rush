#include <unistd.h>

void ft_putchar()
{
	While(tab[][] < [][])
	{
	write(1, tab, 40000);
	}	
}

int rush(int x, int y)
{
	int tabl[200][200];

	tabl[0][0] = 'A';
	tabl[0][199] = 'A';
	tabl[199][0] = 'C';
	tabl[199][199] = 'C';

	ft_putchar();	
}

void main ()
{
	rush(5,5);
	return (0);
}

